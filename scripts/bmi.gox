W = toFloat(argsG[1])
H = toFloat(argsG[2])

pl("体重: %.2f", W)
pl("身高: %.2f", H)

BMI = W / (H * H)

pl("BMI: %.2f", BMI)

if BMI < 18.5 {
	pl("偏瘦")
} elif (18.5 <= BMI) && (BMI < 24) {
	pl("正常")
} elif 24 <= BMI && BMI < 28 {
	pl("偏胖")
} elif 28 <= BMI && BMI < 30 {
	pl("肥胖")
} elif BMI >= 30 {
	pl("重度肥胖")
}